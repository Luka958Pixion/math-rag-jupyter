{
    "python.defaultInterpreterPath": "${workspaceFolder}/.venv/bin/python",
    "python.analysis.extraPaths": [
      "${workspaceFolder}",
      "${workspaceFolder}/.venv/lib/python3.12/site-packages"
    ],
    "python.languageServer": "Pylance",
    "files.exclude": {
      "**/.venv": true,
      "**/.ruff_cache": true,
      "**/.VSCodeCounter": true
    },
    "search.exclude": {
      "**/.venv": true,
      "**/.ruff_cache": true,
      "**/.VSCodeCounter": true
    }
  }
  